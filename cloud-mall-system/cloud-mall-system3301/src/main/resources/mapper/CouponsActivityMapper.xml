<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">




<mapper namespace="com.boot.dao.CouponsActivityMapper">

    <insert id="insertCouponsActivity" parameterType="com.boot.pojo.CouponsActivity">
        insert into coupons_activity values(#{id},#{couponsName},#{couponsCount},#{amount},#{limitCount},#{minPoint},#{startTime},#{endTime},#{note},#{valid})
    </insert>




</mapper>